<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GMHQs" >

    <RelativeLayout
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/background"
        tools:context=".LC" >

        <ImageView
            android:id="@+id/ImageView05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:src="@drawable/logo" />

        <TextView
            android:id="@+id/TextView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/ImageView05"
            android:background="#005daa"
            android:gravity="center"
            android:text="General Mental Health Questions"
            android:textColor="#FFFFFF"
            android:textSize="22dp" />

        <TextView
            android:id="@+id/TextView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/TextView14"
            android:layout_marginTop="12dp"
            android:background="#ffffff"
            android:text="10 being great, 1 being terrible, how would you rate your current wellbeing at the moment?"
            android:textStyle="italic" />

        <EditText
            android:id="@+id/editText1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/TextView15"
            android:background="#ffffff"
            android:ems="10"
            android:inputType="numberDecimal"
            android:text="Answer here."
            android:textSize="12sp" />

        <TextView
            android:id="@+id/TextView01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/editText1"
            android:layout_marginTop="16dp"
            android:background="#ffffff"
            android:text="10 being a great amount, 1 being not at all, how pressure do you feel you have in your life?"
            android:textStyle="italic" />

        <EditText
            android:id="@+id/EditText01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/TextView01"
            android:background="#ffffff"
            android:ems="10"
            android:inputType="numberDecimal"
            android:text="Answer here."
            android:textSize="12sp" />

        <TextView
            android:id="@+id/TextView02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/EditText01"
            android:layout_marginTop="36dp"
            android:background="#ffffff"
            android:text="I am happy mostly."
            android:textStyle="italic" />

        <RadioButton
            android:id="@+id/radioButton1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/TextView02"
            android:layout_marginLeft="81dp"
            android:background="#ffffff"
            android:text="Agree" />

        <TextView
            android:id="@+id/TextView03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/radioButton1"
            android:background="#ffffff"
            android:text="I am constantly depressed."
            android:textStyle="italic" />

        <RadioButton
            android:id="@+id/RadioButton02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/radioButton1"
            android:layout_below="@+id/TextView03"
            android:background="#ffffff"
            android:text="Agree" />

        <RadioButton
            android:id="@+id/RadioButton03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/RadioButton02"
            android:layout_alignBottom="@+id/RadioButton02"
            android:layout_alignLeft="@+id/RadioButton01"
            android:background="#ffffff"
            android:text="Disagree" />

        <TextView
            android:id="@+id/TextView04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/RadioButton02"
            android:background="#ffffff"
            android:text="I have a good group of people in my life that keep me happy and motivated in my life."
            android:textStyle="italic" />

        <RadioButton
            android:id="@+id/RadioButton01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/TextView03"
            android:layout_toRightOf="@+id/RadioButton04"
            android:background="#ffffff"
            android:text="Disagree" />

        <RadioButton
            android:id="@+id/RadioButton04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/RadioButton02"
            android:layout_below="@+id/TextView04"
            android:background="#ffffff"
            android:text="Agree" />

        <RadioButton
            android:id="@+id/RadioButton05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/RadioButton03"
            android:layout_below="@+id/TextView04"
            android:layout_marginLeft="25dp"
            android:background="#ffffff"
            android:text="Disagree" />

    </RelativeLayout>

    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:background="#005daa"
        android:text="Submit"
        android:textColor="#FFFFFF" />

</RelativeLayout>